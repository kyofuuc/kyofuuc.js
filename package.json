  "main": "index.js",
  "types": "index.d.ts",
  "browser": {
    "./lib/connectors/http/httpConnector.js": "./lib/connectors/http/xhrConnector.js",
    "./lib/helpers/node_classes.js": "./lib/helpers/browser_classes.js"
  },
  "jsdelivr": "dist/kyofuuc.min.js",
  "unpkg": "dist/kyofuuc.min.js",
  "typings": "./index.d.ts",
  "scripts": {
